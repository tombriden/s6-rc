#!/bin/execlineb -P

fdmove -c 2 1

foreground { s6-echo -- "\033[35;1m===\033[0m Starting oneshot: init-enp0s31f6" }

foreground { ifconfig enp0s31f6 192.168.1.5 netmask 255.255.255.0 }
if { route add default gw 192.168.1.2 enp0s31f6 }

s6-echo -- "\033[35;1m===\033[0m Done oneshot: init-enp0s31f6"
